.ol-wrapper{
  box-sizing: border-box;
  height:calc(100% - 100rpx);
  padding:20rpx 18rpx 0 18rpx;
  background:#f3f3f3;
}
.ol-list{
  height:100%;
}
.ol-item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:10rpx 30rpx;
  font-size:28rpx;
  background:#fff;
  margin-bottom:15rpx;
  border-radius:10rpx;
  box-shadow:0 2rpx 6rpx 3rpx rgba(0,0,0,0.1);
  line-height:45rpx;
}
.ol-item .item-left{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right:20rpx;
}
.ol-item .item-left view{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.ol-item .item-num{
  line-height:55rpx;
  font-size:30rpx;
}
.ol-item .item-date{
  color:#999;
}
.ol-item .item-right{
  width:110rpx;
  height:40rpx;
  line-height:40rpx;
  text-align: center;
  border-radius:30rpx;
  font-size:26rpx;
}
.ol-item .badge-primary{
  border:1rpx solid #16c4bb;
  color:#16c4bb;
}

.ol-item .badge-default{
  border:1rpx solid #ed5959;
  color:#ed5959;
}
.ol-item .badge-success{
  border:1rpx solid #999;
  color:#999;
}